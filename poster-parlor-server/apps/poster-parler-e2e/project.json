{
  "name": "poster-parler-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["poster-parler"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/poster-parler-e2e/jest.config.cts",
        "passWithNoTests": true
      },
      "dependsOn": ["poster-parler:build", "poster-parler:serve"]
    }
  }
}
